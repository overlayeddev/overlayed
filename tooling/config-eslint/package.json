{
  "name": "lint",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./node.js": {
      "import": "./node.js",
      "require": "./node.js"
    },
    "./next.js": {
      "import": "./next.js",
      "require": "./next.js"
    }
  },
  "files": [
    "node.js",
    "next.js",
    "package.json"
  ],
  "scripts": {
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json}\" --check"
  },
  "dependencies": {
    "@eslint/js": "^9.5.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.4",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.9.0",
    "tsconfig": "workspace:*",
    "typescript-eslint": "8.32.0"
  }
}
